        "ext-dom": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "guzzlehttp/guzzle": "^7.4.2",
        "symfony/console": "^4.2",
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "symfony/requirements-checker": "^2.0"
    },
    "require-dev": {
